{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "^/mystoreapi(/.*)?$", "dest": "/api/index.js" },
    {
      "src": "^/(.*\\.(js|css|png|jpg|svg|webp|json|txt|ico|woff2|woff|ttf))$",
      "dest": "/$1"
    },
    { "src": "^/(.*)", "dest": "/index.html" }
  ]
}
